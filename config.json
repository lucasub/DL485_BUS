# Descrizione PLC
# All'interno delle singole board è presente un PLC che può essere attivato tramite la funzione desiderata (es. toggle_on)

#    plc_function:       => Funzione associata al PLC relativa a quell'uscita
#       disable         => PLC disabilitato
#       equal           => funzione uguale. Ad esempio uscita = ingresso
#       and             => funzione AND
#       or              => funzione OR
#       xor             => funzione XOR
#       odd             => funzione dispari. Uscita a 1 se gli ingressi a 1 sono dispari
#       toggle_on       => fa il toggle sul fronte ON dell'ingresso
#       toggle_off      => fa il toggle sul fronte OFF dell'ingresso
#       toggle_on_off   => fa il toggle sul fronte OFF e ON dell'ingresso
#       timer           => timer uscita
#       power_on        => Uscita per comandare il rele di reset alimentazione RAPBERRY quando non risponde sulla seriale per un certo tempo.

#    inverted            => uscita negata
#        0               => non invertita
#        1               => invertita
#

{//inizio file
    
    "TYPEB":{//riassunto dati per campo boardtype, Tipi di Board disponibili. Non toccare

        "DL485M": 1,
        "DL485B": 2,
        "DL485Pold": 3, //scheda tipo 3 vecchia sostituita con 5
        "DL485R": 4,
        "DL485P": 5,

        "1": "DL485M",
        "2": "DL485B", 
        "3": "DL485Pold",
        "4": "DL485R",
        "5": "DL485P"
    },
    
    "GENERAL_NET":{// Configurazione generale del BUS
        "bus_port": "/dev/ttyAMA0", # Porta seriale Raspberry PI
        "bus_baudrate": 19200, // Velocità BUS e seriale
        "max_board_address": 24, // Numero massimo di Board presenti sul BUS
        "board_address": 2 // ID assegnato a Raspberry. Deve avere l'ID della scheda collegata alla seriale di Raspberry PI +1
    },
    
    "BOARD1":{//da finire
        "GENERAL_BOARD":{// Configurazione generale della BOARD
            "enable": 1, // Abilitazione della BOARD: 0=disabilitata, 1=abilitata
            "name": "SCHEDA MASTER", // Nome della BOARD
            "board_type": 1 // Tipo di Board: Vedi TYPEB
        },

        "VIN":{// Tensione alimentazione BOARD
            "enable": 1, // Valori: 0 disabilitato, 1 abilitato. Abilitazione dell'IO
            "logic_io": 1, // Valori interi. IO LOGICO che deve essere UNIVOCO per ciascun IO della stessa BOARD
            "time_refresh": 600, // Valori: 0...16382 decimi di secondo, 16384 MAI. Tempo di ritrasmissione del valore dell'IO sul BUS. 600 = 60 secondi
            "device_type": "VINR1R2",
            "rvcc": 12000, // Valore resistenza posta tra IO e ingresso alimentazione.
            "rgnd": 470, // Valore resistenza posta tra IO e 0V.
            "name": "Vin", // Nome che verrà riportato su DOMOTICZ
            "description": "Tensione alimentazione scheda" // Descrizione che verrà riportato su DOMOTICZ
        },

        "TEMP_ATMEGA":{
            "enable": 1,
            "logic_io": 2,
            "time_refresh": 600,
            "device_type": "TEMP_ATMEGA",
            "name": "TEMP_ATMEGA",
            "description": "Temperatura Board ATMEGA328PB"
        }, 
        
    "OUT1":
    {
        "enable": 1,
        "logic_io": 3,
        "device_type": "DIGITAL_OUT",
        "default_startup_value": 0,
        "inverted": 0,
        "filter": 127,
        "time_refresh": 16383,
        "n_refresh_on": 1,
        "n_refresh_off": 1,
        "plc_function": "power_on",
        "plc_linked_board_id_logic_io" :["*-1"],
        "power_on_timeout": 150,   //3000: 300 secondi di assenza seriale prima di spegnere
        "power_on_tminoff": 30, //30: 3 secondi di tempo minimo off rasperry
        "power_on_voltage_off": 11.4, //11.4: soglia tensione bassa, si comanda spegnimento raspberry
        "power_on_voltage_on": 12.8, //12.8: soglia alta, si comanda accensione se raspberry era spento
        "name": "BUS OUT RESET",
        "description": "BUS Uscita RELE RESET RASPBERRY",
        "dtype": "Switch"
    }
        
    },//fine board

  // Definizione della BOARD con ID n.3. Sul BUS ogni Board deve avere un ID univoco
  "BOARD5":{  // EX5
    // Configurazione generale della BOARD
    "GENERAL_BOARD":{
        "name": "SCHEDA CONTATORE", // Nome della BOARD
        "board_type": 5, // Tipo di Board: Vedi TYPEB
        "enable": 1 // Abilitazione della BOARD: 0=disabilitata, 1=abilitata
    },

    // Tensione alimentazione BOARD

    "VIN":{// Tensione alimentazione BOARD
        "enable": 1, // Valori: 0 disabilitato, 1 abilitato. Abilitazione dell'IO
        "logic_io": 1, // Valori interi. IO LOGICO che deve essere UNIVOCO per ciascun IO della stessa BOARD
        "time_refresh": 60, // Valori: 0...16382 decimi di secondo, 16384 MAI. Tempo di ritrasmissione del valore dell'IO sul BUS. 600 = 60 secondi
        "device_type": "VINR1R2",
        "rvcc": 12000, // Valore resistenza posta tra IO e ingresso alimentazione.
        "rgnd": 470, // Valore resistenza posta tra IO e 0V.
        "name": "Vin", // Nome che verrà riportato su DOMOTICZ
        "description": "Tensione alimentazione scheda" // Descrizione che verrà riportato su DOMOTICZ
    },

    "TEMP_ATMEGA":{
        "enable": 1,
        "logic_io": 2,
        "time_refresh": 600,
        "device_type": "TEMP_ATMEGA",
        "name": "TEMP_ATMEGA",
        "description": "Temperatura Board ATMEGA328PB"
    },
    
    "PE2":{ // Tensione analogica rilavata dal TA
        "enable": 1,     
        "logic_io": 10,
        "device_type": "VINKMKA",
        "kmul": 1,
        "kadd": 0,
        "time_refresh": 300,      
        "name": "Corrente",
        "description": "Misura corrente",
        "dtype": "Custom Sensor"
    },

    "PB1":{ // Fotoresistenza Counter
        "enable": 1,
        "logic_io": 11,
        "device_type": "DIGITAL_IN_PULLUP",
        "time_refresh": 300,
        "n_refresh_on": 1,
        "n_refresh_off": 1,
        "filter": 5,
        "inverted": 1,
        "name": "FOTO R",
        "description": "Counter Incremental",
        "dtype": "Counter Incremental"
    },

    "PC0":{
        "enable": 1,
        "logic_io": 12,
        "device_type": "DIGITAL_OUT",
        "time_refresh": 200,       
        "plc_function": "counter_up",
        "plc_linked_board_id_logic_io": ["*-11"],
        "counter_mode": 0,
        "counter_filter": 0, // non usato
        "timeout_counter": 65535, // escluso
        "name": "WATTORA",
        "description": "WATT per ORA",
        "dtype": "kWh"
    },

    "PC1":{
        "enable": 1,
        "logic_io": 13,
        "device_type": "DIGITAL_OUT",
        "default_startup_value": 0,
        "inverted": 1,
        "filter": 10,
        "time_refresh": 200,
        "n_refresh_on": 1,
        "n_refresh_off": 1,
        "plc_function": "powermeter",
        "counter_mode": 27, // b11011 memoria del valore precendente e aggiorna dato a periodo ottenuto, azzera tempo ad ogni start a conteggio finito. Considera end come nuovo start, end su fronte ON
        "powermeter_k": 60000, // numeratore (nel caso del contatore enel, il risultato è 1/6 della potenza attuale)
        "kmul": 6,
        "kadd": 0,
        "counter_filter": 0, // non usato
        "counter_timeout": 500, // 5 secondi
        "plc_linked_board_id_logic_io": ["*-11"],
        "name": "POWER METER",
        "description": "POWER METER",
        "dtype": "kWh"
    },   

    "PC2":{
        "enable": 1,
        "logic_io": 14,
        "device_type": "DIGITAL_OUT",
        "default_startup_value": 0,
        "inverted": 1,
        "filter": 127,
        "time_refresh": 200,
        "n_refresh_on": 1,
        "n_refresh_off": 1,
        "plc_function": "time_meter",
        "counter_mode": 27, // b11011 
        "counter_filter": 0, // non usato
        "counter_timeout": 65535, // 5 secondi
        "plc_linked_board_id_logic_io": ["*-11"],
        "name": "time_meter",
        "description": "time_meter",
        "dtype": "Current/Ampere"
    },
         
    "PB2":{
        "enable": 1,
        "logic_io": 3,
        "device_type": "DS18B20",
        "pullup": 1,
        "time_refresh": 600,
        "write_ee" : [],
        "name": "DS18B20",
        "description": "Sensore Temperatura",
        "dtype": "Temperature"
    },

    "PD4":{
        "enable": 0,
        "logic_io": 4,
        "device_type": "DS18B20",
        "pullup": 1,
        "time_refresh": 600,
        "write_ee" : [],
        "name": "DS18B20",
        "description": "Sensore Temperatura",
        "dtype": "Temperature"
    },

    "BME280":{
        "enable": 0,
        "logic_io": 6,
        "device_type": "BME280",
        "time_refresh": 600,
        "address": "0xEC",
        "name": "BME280",
        "description": "Sensore T+U+B bagno",
        "dtype": "Temp+Hum+Baro"
    },

    "TSL2561":{
        "enable": 0,
        "logic_io": 7,
        "device_type": "TSL2561",
        "time_refresh": 600,
        "address": "0x72",
        "name": "TSL2561",
        "description": "Sensore Luminosità",
        "dtype": "Light"
    },
    
    "AM2320":{
        "enable": 0,
        "logic_io": 8,
        "device_type": "AM2320",
        "time_refresh": 30,
        "address": "0xB8",
        "name": "AM2320",
        "description": "Sensore T+U bagno",
        "dtype": "Temp+Hum"
    }

},

    
